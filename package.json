{
  "name": "test_app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ios:dev:iphone": "pnpm kill-port && pnpm ios:set-sim && open -a Simulator && pnpm tauri ios dev \"iPhone 16e\"",
    "ios:dev:ipad": "pnpm kill-port && pnpm ios:set-sim && open -a Simulator && pnpm tauri ios dev \"iPad (A16)\"",
    "ios:real:device": "pnpm kill-port && pnpm ios:set-device && pnpm tauri ios dev --host 192.168.8.151 \"Rasmus Friis - iPad\"",
    "ios:set-sim": "bash scripts/set-ios-target.sh simulator",
    "ios:set-device": "bash scripts/set-ios-target.sh device",
    "kill-port": "lsof -ti:1420 | xargs kill -9 2>/dev/null || true",
    "tauri:dev": "pnpm kill-port && tauri dev",
    "dev": "pnpm kill-port && vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "pnpm kill-port && vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.4.7",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-sql": "^2.3.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tauri-apps/cli": "^2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  }
}
