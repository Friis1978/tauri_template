    "ios:dev:ipad": "pnpm kill-port && printf '0\n' | pnpm tauri ios dev",
    "tauri:dev": "pnpm kill-port && tauri dev",
    "dev": "pnpm kill-port && vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "pnpm kill-port && vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
